<svg width="400" height="300" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="richGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#2c2c2c;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a1a1a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="accentGrad" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#d4af37;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#b8941f;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="400" height="300" fill="url(#richGrad)"/>
  
  <!-- Header with blur effect -->
  <rect x="0" y="0" width="400" height="50" fill="#000000" opacity="0.3"/>
  <rect x="20" y="15" width="60" height="20" fill="#ffffff" rx="2"/>
  <rect x="320" y="10" width="60" height="30" fill="url(#accentGrad)" rx="15"/>
  
  <!-- Dynamic Hero -->
  <rect x="30" y="70" width="340" height="120" fill="#ffffff" opacity="0.05" rx="12"/>
  <rect x="50" y="90" width="250" height="25" fill="#ffffff" rx="3"/>
  <rect x="50" y="125" width="180" height="12" fill="#d4af37" rx="2"/>
  <rect x="50" y="145" width="140" height="12" fill="#ffffff" opacity="0.7" rx="2"/>
  <rect x="50" y="165" width="100" height="30" fill="url(#accentGrad)" rx="15"/>
  
  <!-- Interactive Elements -->
  <rect x="280" y="80" width="80" height="80" fill="#d4af37" opacity="0.2" rx="8"/>
  <circle cx="320" cy="120" r="20" fill="#ffffff" opacity="0.1"/>
  
  <!-- Content Grid -->
  <rect x="20" y="210" width="80" height="70" fill="#ffffff" opacity="0.1" rx="6"/>
  <rect x="110" y="210" width="80" height="70" fill="#d4af37" opacity="0.2" rx="6"/>
  <rect x="200" y="210" width="80" height="70" fill="#ffffff" opacity="0.1" rx="6"/>
  <rect x="290" y="210" width="80" height="70" fill="#d4af37" opacity="0.2" rx="6"/>
  
  <!-- Animated elements -->
  <circle cx="50" cy="50" r="8" fill="#d4af37" opacity="0.6"/>
  <circle cx="350" cy="250" r="12" fill="#ffffff" opacity="0.3"/>
  <rect x="370" y="60" width="20" height="20" fill="#d4af37" opacity="0.4" rx="10" transform="rotate(45 380 70)"/>
  
  <text x="200" y="20" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#ffffff" font-weight="bold">Rich & Interactive</text>
</svg>